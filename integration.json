{
  "data": {
    "date": {
      "created_at": "2025-02-21",
      "updated_at": "2025-02-21"
    },
    "descriptions": {
      "app_description": "Retrieve IT asset details from Google Sheets and send them to a Telex channel.",
      "app_logo": "https://your-logo-url.com/logo.png",
      "app_name": "IT Asset Management",
      "app_url": "https://your-integration-url.com",
      "background_color": "#4A90E2"
    },
    "integration_category": "IT Service Management",
    "integration_type": "output",
    "is_active": true,
    "key_features": [
      "Retrieve IT asset information from a Google Sheet using Telex queries.",
      "Integrates with Google Sheets API for real-time asset lookups.",
      "Supports detailed and summary response formats.",
      "Secure API key authentication for controlled access."
    ],
    "permissions": {
      "it_admin": {
        "always_online": true,
        "display_name": "IT Asset Manager"
      }
    },
    "settings": [
      {
        "label": "Google Sheets API Key",
        "type": "text",
        "description": "API Key for accessing Google Sheets.",
        "required": true,
        "default": ""
      },
      {
        "label": "Sheet ID",
        "type": "text",
        "description": "The Google Sheet ID containing asset data.",
        "required": true,
        "default": ""
      },
      {
        "label": "Asset Lookup Column",
        "type": "text",
        "description": "Column name where asset queries should be matched.",
        "required": true,
        "default": "Service Tag"
      },
      {
        "label": "Response Format",
        "type": "dropdown",
        "description": "Format for returning asset data.",
        "required": true,
        "default": "Detailed",
        "options": ["Detailed", "Summary"]
      }
    ],
    "output": {
      "webhook": {
        "target_url": "https://your-api-url.com/api/telex/webhook",
        "http_method": "POST",
        "headers": {
          "Content-Type": "application/json",
          "Authorization": "Bearer {{api_key}}"
        },
        "body": {
          "status": "success",
          "asset_info": {
            "status": "{{status}}",
            "previous_user": "{{previous_user}}",
            "current_user": "{{current_user}}",
            "hostname": "{{hostname}}",
            "service_tag": "{{service_tag}}",
            "laptop_model": "{{laptop_model}}",
            "location": "{{location}}"
          },
          "return_url": "{{return_url}}"
        }
      }
    }
  }
}
